{
  "name": "n8n-nodes-guardspine",
  "version": "0.1.0",
  "description": "n8n nodes for GuardSpine governance layer",
  "license": "MIT",
  "author": "David Youssef",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "eslint . --ext .ts",
    "test": "jest",
    "prepublishOnly": "npm run build"
  },
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/GuardSpineApi.credentials.js"
