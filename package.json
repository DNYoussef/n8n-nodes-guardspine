      "dist/nodes/CodeGuard/CodeGuard.node.js",
      "dist/nodes/BeadsCreate/BeadsCreate.node.js",
      "dist/nodes/ApprovalWait/ApprovalWait.node.js",
      "dist/nodes/BeadsUpdate/BeadsUpdate.node.js",
      "dist/nodes/EvidenceSeal/EvidenceSeal.node.js",
      "dist/nodes/CouncilVote/CouncilVote.node.js"
    ]
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "n8n-workflow": ">=1.0.0"
  }
}
